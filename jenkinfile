pipeline {
    agent any
    stages {
        stage ('latest stage') {
            steps {
              git 'https://github.com/nakkasrikrishna/springboot-sample-app.git'
            }
        }
       stage ('Build stage') {
            steps {
                sh 'mvn  clean package'
            }
        }
        stage ('Test stage') {
            steps {
                sh 'mvn test'
            }
        } 
       
    }
}
